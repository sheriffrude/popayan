@model DetalleTareasViewModel
@{
    ViewBag.Title = "Detalle Tarea";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

<div class="modal-body">
    @Html.HiddenFor(m => m.ClienteId)
    <div class="help-block"></div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <table id="tabla-Ots" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
            <thead>
                <tr>
                    <th class="text-center">Número OT</th>
                    <th class="text-center">Referencia OT</th>
                    <th class="text-center"># Tarea</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    <div class="help-block"></div>
    <div class="clearfix"></div>
</div>


<a href="@Url.Action("DetalleTareas", "Consolidado", new { Area = "Trafico" })" data-toggle="modal" data-target="#" data-execute-onload="OnLoadDetalleTarea" data-size="all" id="Modal-detalle-tarea"class="hidden"></a>
