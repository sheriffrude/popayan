@model TareaListarCambiosFechaHoraEntregaViewModel
@{
    ViewBag.Title = $"Historial de cambios fecha y hora de entrega - Tarea {Model.Numero}";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title"> @ViewBag.Title</h4>
</div>

<div class="modal-body">
    <div class="col-md-12">
        @Html.HiddenFor(m => m.TareaId)
        <table id="tabla_tarea_lista_cambios_fecha_hora_entrega" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
            <thead>
                <tr>
                    <th class="text-center">Fecha Anterior</th>
                    <th class="text-center">Hora Anterior</th>
                    <th class="text-center">Fecha Nueva</th>
                    <th class="text-center">Hora Nueva</th>
                    <th class="text-center">Observaciones</th>
                    <th class="text-center">Persona que realizo la modificación</th>
                    <th class="text-center">Fecha y hora modificación</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div class="col-md-12 text-right">
        <a href="@Url.Action("Consultar", "Tarea", new { Area = "Trafico", id = Model.TareaId })" class="btn btn-secondary" data-toggle="modal" data-target="#" data-size="lg" data-execute-onload="TAREA_CONSULTAR.OnLoad">Volver</a>
        <button type="button" class="btn btn-warning" data-dismiss="modal">Cerrar</button>
    </div>
    <div class="clearfix"></div>
</div>