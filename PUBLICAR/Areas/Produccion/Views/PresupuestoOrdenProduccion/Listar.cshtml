@{
    ViewBag.Title = "Órdenes de producción";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

<div class="modal-body">
    <div class="col-md-12">
        <div class="pull-left">
            <form id="form_filtro_tabla_ordenes_produccion" class="form-inline">
                <div class="form-group">
                    <input type="search" name="input-filtro" id="input_filtro_listar_ordenes_produccion" class="form-control" placeholder="Filtro" />
                    <input type="submit" class="btn btn-default" value="BUSCAR" />
                    <input type="button" class="btn btn-default" value="VER TODO" onclick="PresupuestoOrdenProduccionListar.ResetearTabla()" />
                </div>
            </form>
        </div>
    </div>
    <div class="col-md-12">
        <table id="tabla_ordenes_produccion" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
            <thead>
                <tr>
                    <th class="text-center">#</th>
                    <th class="text-center">Version Interna</th>
                    <th class="text-center">Version Externa</th>
                    <th class="text-center">Proveedor</th>
                    <th class="text-center">Forma de Pago</th>
                    <th class="text-center">Fecha entrega</th>
                    <th class="text-center">Fecha Radicación</th>
                    <th class="text-center">Vigencia Inicial</th>
                    <th class="text-center">Vigencia Final</th>
                    <th class="text-center">Lugar entrega</th>
                    <th class="text-center">Creado por</th>
                    <th class="text-center">Fecha Hora Creación</th>
                    <th class="text-center">Estado</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div class="clearfix"></div>
</div>

<div class="col-md-12">
    <div id="reportViewer1" class="report"></div>
</div>

