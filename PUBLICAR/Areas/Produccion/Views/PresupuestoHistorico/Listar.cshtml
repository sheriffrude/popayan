@{
    ViewBag.Title = "Ver Historico";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">Historico del presupuesto</h4>
</div>

<div class="modal-body">
    <div class="row">
        <div class="col-md-12">
            <table id="tabla_presupuesto_historico" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
                <thead>
                    <tr>
                        <th class="text-center">Version Interna</th>
                        <th class="text-center">Version Externa</th>
                        <th class="text-center">Referencia</th>
                        <th class="text-center">Vigencia Inicial</th>
                        <th class="text-center">Vigencia Final</th>
                        <th class="text-center">Nota Adicional</th>
                        <th class="text-center">Número Aprobacion</th>
                        <th class="text-center">Lugar Ejecución</th>
                        <th class="text-center">Dirigido A</th>
                        <th class="text-center">Observaciones</th>
                        <th class="text-center">Estado Presupuesto</th>
                        <th class="text-center">Persona Modificacion</th>
                        <th class="text-center">Fecha y Hora Modificacion</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
</div>