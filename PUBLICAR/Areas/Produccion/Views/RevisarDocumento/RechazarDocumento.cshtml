@{
    ViewBag.Title = "Observación de Devolución";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

<div class="modal-body">
  
                        <div>
                            @Html.ValidationMessage("Error", new { @class = "text-danger" })
                            @Html.ValidationMessage("Success", new { @class = "text-success" })
                        </div>

                        <div class="col-md-12">
                            <div class="form-inline">
                                <div class="text-center">
                                    <div class="col-md-4">
                                        <label class="control-label"># Documento</label>
                                    </div>
                                    <div class="col-md-8">
                                        <label class="control-label">Observación</label>
                                    </div>
                                    <div class="text-center" id="rechazar-factura-id">

                                    </div>
                                </div>


                            </div>


                            <div class="text-right">
                                <button type="button" class="btn btn-success" onclick="RechazarDocumentos()">GUARDAR</button>
                                <button type="button" class="btn btn-warning" data-dismiss="modal">CANCELAR</button>
                            </div>
                        </div>
                        <div class="clearfix"></div>

        <!-- END Modal rechazar documento -->
</div>
