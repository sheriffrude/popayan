@model AprobacionLegalizacionListarJerarquiaViewModel
@{
    ViewBag.Title = "Jerarquía de Aprobación Legalización";
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

<div class="modal-body">
    @Html.HiddenFor(m => m.LegalizacionId)
    <!--Tabla-->
    <div class="row"> 
        <div class="col-md-12">
            <table id="tabla_jerarquia_aprobacion" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
                <thead>
                    <tr>
                        <th class="text-center">Orden</th>
                        <th class="text-center">Grupo</th>
                        <th class="text-center">Persona</th>
                        <th class="text-center">Status</th>
                        <th class="text-center">Observación</th>
                        <th class="text-center">Fecha y Hora</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
    <div class="clearfix"></div>
</div>