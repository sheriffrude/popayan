@model VerFacturaClienteViewModel
@{
    ViewBag.Title = "Facturas Presupuesto " + Model.PresupuestoId;
}

<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

@Html.HiddenFor(m => m.PresupuestoId);

<div class="modal-body">
    <div class="row">
        <div class="col-md-12">
            <table id="tabla-historico-factura-cliente" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
                <thead>
                    <tr>
                        <th class="text-center"># Factura</th>
                        <th class="text-center">$ Valor</th>
                        <th class="text-center">$ Impuesto</th>
                        <th class="text-center">$ Total</th>
                        <th class="text-center">Fecha Registro</th>
                        <th class="text-center">Registrada Por</th>
                        <th class="text-center">Fecha de Rechazo</th>
                        <th class="text-center">Hora de rechazo</th>
                        <th class="text-center">Rechazada Por</th>
                        <th class="text-center">Estado</th>
                    </tr>
                </thead>
                <tbody></tbody>
            </table>
        </div>
    </div>
    <div class="clearfix"></div>
    <div class="pull-right">
        <div class="col-md-12">
            <label class="control-label">VALOR PENDIENTE POR FACTURAR</label>
            <input class="form-control text-center" id="valor-total-pendiente-facturar" readonly />
        </div>
    </div>
    <div class="pull-right">
        <div class="col-md-12">
            <label class="control-label">VALOR TOTAL FACTURAS ACTIVAS</label>
            <input class="form-control text-center" id="valor-total-factura" readonly />
        </div>
    </div>
    <div class="clearfix"></div>
    <hr />
    <div class="clearfix"></div>



</div>
