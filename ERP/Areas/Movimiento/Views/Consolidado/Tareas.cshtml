@model ProductosViewModel
@{
    ViewBag.Title = "OTS POR DEPARTAMENTO";
}


@Html.HiddenFor(m => m.Id)
<div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
    <h4 class="modal-title">@ViewBag.Title</h4>
</div>

<div class="modal-body">

    <div class="col-md-12">
        <div id="chartContainer_Clientes_Deptos"></div>
    </div>

    <div class="help-block"></div>
    <div class="clearfix"></div>
    <div class="col-md-12">
        <table id="tabla-Deptos" class="table table-striped table-bordered compact text-center" cellpadding="0" width="100%">
            <thead>
                <tr>
                    <th class="text-center">Tareas</th>
                    <th class="text-center">Departamento</th>
                    <th class="text-center">Número Ots</th>
                </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>

    <div class="help-block"></div>
    <div class="clearfix"></div>
</div>


